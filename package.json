{
  "name": "acp-integration-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for integrating AI agents with the Agent Commerce Protocol (ACP) on Virtuals",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:mock": "ENABLE_MOCK_BUYER=true tsx src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "setup": "npm install && cp .env.example .env"
  },
  "keywords": [
    "ACP",
    "Agent Commerce Protocol",
    "Virtuals",
    "AI Agent",
    "Web3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@virtuals-protocol/acp-node": "^0.1.0-beta.15",
    "@virtuals-protocol/game-acp-plugin": "^0.2.6",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.15",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}